            <action id="act2">
              <title>
                <en><![CDATA[Use]]></en>
                <de><![CDATA[Benutze]]></de>
              </title>
              <dialog>
                <dia puppet="_avatar" frame="" sound="voice/boathouse_window_voice.swf:boathouse_window0010">
                  <en><![CDATA[If only I were tall enough to reach that hatch!]]></en>
                  <de><![CDATA[Wenn ich nur groß genug wäre, um die Luke zu erreichen!]]></de>
                </dia>
              </dialog>
              <script>
                <dialogue play="*" />
              </script>
            </action>
